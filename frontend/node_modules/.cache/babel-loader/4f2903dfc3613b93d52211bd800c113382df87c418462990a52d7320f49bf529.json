eLogout","profileRedirect","className","children","to","src","alt","username","Provider","value","onClick"],"sources":["/home/ubuntu/EmberLight_Projecte2/frontend/src/components/AppNavbar.js"],"sourcesContent":["import { Link, useNavigate } from \"react-router-dom\";\nimport { FaUser } from \"react-icons/fa\";\nimport { IconContext } from \"react-icons\";\nimport \"./AppNavbar.css\";\nimport { useAuth } from \"../contexts/AuthContext\";\nimport { useState } from \"react\";\n\nconst AppNavbar = () => {\n  const { user, logout } = useAuth();\n  const [dropdownOpen, setDropdownOpen] = useState(false);\n  const navigate = useNavigate();\n\n  const toggleDropdown = () => {\n    setDropdownOpen(!dropdownOpen);\n  };\n\n  const handleLogout = () => {\n    logout();\n    setDropdownOpen(false)\n    navigate(\"/login\");\n  };\n\n  const profileRedirect = () => {\n    setDropdownOpen(false)\n    navigate(\"/profile\");\n  }\n\n  return (\n    <nav className=\"navbar\">\n      <div className=\"nav-left\">\n        <Link to=\"/journal\" className=\"logo-link\">\n          <img src=\"/logoEmberLight.jpg\" alt=\"Logo\" className=\"logo\"></img>\n        </Link>\n        <Link to=\"/journal\" className=\"nav-link\">DIARIO</Link>\n        <Link to=\"/chat\" className=\"nav-link\">CHAT</Link>\n      </div>\n      <div className=\"nav-right\">\n      {user && <span className=\"user\">Bienvenido, {user.username}!</span>}\n        <IconContext.Provider\n          value={{ className: \"nav-icon\" }}\n        >\n          <div className=\"user-icon\" onClick={toggleDropdown}>\n            <FaUser />\n          </div>\n        </IconContext.Provider>\n        {dropdownOpen && (\n          <div className=\"dropdown\">\n            <div className=\"profile-option\" onClick={profileRedirect}>Perfil</div>\n            <div className=\"logout-option\" onClick={handleLogout}>Logout</div>\n          </div>\n        )}\n      </div>\n    </nav>\n  );\n};\n\nexport default AppNavbar;\n"],"mapping